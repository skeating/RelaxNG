<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" xmlns:arrays="http://www.sbml.org/sbml/level3/version1/arrays/version1" level="3" version="1" arrays:required="true">
  <model>
    <listOfParameters>
      <!-- parameters used as array sizes -->
      <parameter id="n" value="8" constant="true"/>
      <parameter id="m" value="5" constant="true"/>
      <parameter id="o" value="3" constant="true"/>
      <parameter id="x" constant="true">
        <arrays:orderedListOfDimensions>
          <arrays:dimension arrays:id="i" arrays:size="n"/>
        </arrays:orderedListOfDimensions>
      </parameter>
      <parameter id="y" constant="true"/>
    </listOfParameters>
    <listOfRules>
      <rateRule variable="x">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <ci> y </ci>
        </math>
        <arrays:orderedListOfDimensions>
          <arrays:dimension arrays:id="i" arrays:size="m"/>
        </arrays:orderedListOfDimensions>
        <arrays:orderedListOfIndices>
          <arrays:index>
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <selector/>
                <ci> x </ci>
                <ci> i </ci>
              </apply>
            </math>
          </arrays:index>
        </arrays:orderedListOfIndices>
      </rateRule>
      <rateRule variable="x">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn type="integer"> 2 </cn>
            <ci> y </ci>
          </apply>
        </math>
        <arrays:orderedListOfDimensions>
          <arrays:dimension arrays:id="i" arrays:size="o"/>
        </arrays:orderedListOfDimensions>
        <arrays:orderedListOfIndices>
          <arrays:index>
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <selector/>
                <ci> x </ci>
                <apply>
                  <plus/>
                  <ci> i </ci>
                  <cn> 5 </cn>
                </apply>
              </apply>
            </math>
          </arrays:index>
        </arrays:orderedListOfIndices>
      </rateRule>
    </listOfRules>
  </model>
</sbml>
