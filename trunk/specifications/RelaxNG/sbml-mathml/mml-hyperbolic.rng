   </element>
   <element name="cosh">
    <ref name="cosh.datatype"></ref>
   </element>
   <element name="tanh">
    <ref name="tanh.datatype"></ref>
   </element>
   <element name="sech">
    <ref name="sech.datatype"></ref>
   </element>
   <element name="csch">
    <ref name="csch.datatype"></ref>
   </element>
   <element name="coth">
    <ref name="coth.datatype"></ref>
   </element>
   <element name="arcsinh">
    <ref name="arcsinh.datatype"></ref>
   </element>
   <element name="arccosh">
    <ref name="arccosh.datatype"></ref>
   </element>
   <element name="arctanh">
    <ref name="arctanh.datatype"></ref>
   </element>
   <element name="arcsech">
    <ref name="arcsech.datatype"></ref>
   </element>
   <element name="arccsch">
    <ref name="arccsch.datatype"></ref>
   </element>
   <element name="arccoth">
    <ref name="arccoth.datatype"></ref>
   </element>
  </choice>
 </define>
 
</grammar>
