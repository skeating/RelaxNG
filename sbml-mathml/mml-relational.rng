<?xml version="1.0" encoding="UTF-8"?>

<grammar xmlns="http://relaxng.org/ns/structure/1.0"
 datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">

 <define name="eq.datatype">
  <ref name="mathml_attrib"/>
 </define>
 
 <define name="neq.datatype">
  <ref name="mathml_attrib"/>
 </define>
 
 <define name="gt.datatype">
  <ref name="mathml_attrib"/>
 </define>

 <define name="lt.datatype">
  <ref name="mathml_attrib"/>
 </define>
 
 <define name="geq.datatype">
  <ref name="mathml_attrib"/>
 </define>
 
 <define name="leq.datatype">
  <ref name="mathml_attrib"/>
 </define>
 
 <define name="relational" combine="choice">
  <choice>
   <element name="eq">
    <ref name="eq.datatype"></ref>
   </element>
   <element name="neq">
    <ref name="neq.datatype"></ref>
   </element>
   <element name="gt">
    <ref name="gt.datatype"></ref>
   </element>
   <element name="lt">
    <ref name="lt.datatype"></ref>
   </element>
   <element name="geq">
    <ref name="geq.datatype"></ref>
   </element>
   <element name="leq">
    <ref name="leq.datatype"></ref>
   </element>   
  </choice>
 </define>
 
</grammar>
